{% extends 'base.html.twig' %}

{% block title %}Enjeu index{% endblock %}

{% block body %}
    <h1>Enjeu index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DescriptionEnjeu</th>
                <th>TypeEnjeu</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for enjeu in enjeus %}
            <tr>
                <td>{{ enjeu.id }}</td>
                <td>{{ enjeu.DescriptionEnjeu }}</td>
                <td>{{ enjeu.TypeEnjeu }}</td>
                <td>
                    <a href="{{ path('enjeu_show', {'id': enjeu.id}) }}">show</a>
                    <a href="{{ path('enjeu_edit', {'id': enjeu.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('enjeu_new') }}">Create new</a>
{% endblock %}
